{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-plugin-prettier"
    ],
    "jsRules": {},
    "rules": {
        "class-name": false,
        "max-classes-per-file": 90000,
        "no-bitwise": false,
        "no-conditional-assignment": false,
        "no-string-throw": false,
        "prettier": [true,
            {
                "tabWidth":4,"trailingComma":"all"
            }
        ],
        "quotemark": false,
        "variable-name": false
    },
    "rulesDirectory": []
}
